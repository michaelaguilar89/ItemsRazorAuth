@page
@model ItemsRazorAuth.Pages.Users.IndexModel
@{
}
<div class="container">
    <div class="row">
        <div class="col-md-6 mx-auto">
            <h3 class="text-center">List Of Users</h3>
            <table class="table " style="background-color:aliceblue">
                <thead>
                    <tr>
                        <th>User</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach(var u in Model.Users){
                        <tr>
                            <td>@u.UserName</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
